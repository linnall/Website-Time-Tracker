{
    "name": "tohack",
    "version": "1.0",
    "description": "Let's you time how long you've been on certain websites",
    "permissions": ["tabs", "activeTab", "storage", "declarativeContent"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    
    "browser_action":{
        "default_popup": "popup.html"
    },
  
    "manifest_version": 2
  }